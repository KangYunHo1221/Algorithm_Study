    "## 123 더하기\n",
    "## https://www.acmicpc.net/problem/9095\n",
    "\n",
    "T = int(input())\n",
    "\n",
    "dp = [0]*12\n",
    "dp[0] = 1\n",
    "\n",
    "for i in range(12):\n",
    "    for j in range(1,4):\n",
    "        if i-j >= 0:\n",
    "            dp[i] += dp[i-j]\n",
    "\n",
    "for _ in range(T):\n",
    "    n = int(input())\n",
    "    print(dp[n])\n",
    "\n"
   ],
