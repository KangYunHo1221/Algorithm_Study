    "## 물병\n",
    "## https://www.acmicpc.net/problem/1052\n",
    "import sys\n",
    "sys.setrecursionlimit(10**9) #재귀 제한 풀기 위해서\n",
    "\n",
    "n, k = map(int, input().split())\n",
    "\n",
    "answer = 0\n",
    "while bin(n).count('1') > k:\n",
    "    plus = 2**(bin(n)[::-1].index('1'))\n",
    "    answer += plus\n",
    "    n += plus"
